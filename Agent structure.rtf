\f0\fs24 \cf0 agents:\
  - name: bp-data-analyzer\
    description: >\
      Analyzes blood pressure readings to identify patterns, trends,\
      risk levels, and variability using objective clinical logic.\
    model: claude-sonnet-4-20250514\
    temperature: 0.3\
    max_tokens: 2000\
    function: |\
      - Calculate averages, ranges, and variability of BP readings\
      - Detect trends (improving, stable, deteriorating)\
      - Identify concerning and positive patterns\
      - Classify BP risk levels\
      - Produce structured data insights for downstream agents\
    tools_used:\
      - statistical_analysis\
      - pattern_detection\
      - hypertensive_risk_classification\
      - structured_json_output\
\
  - name: bp-safety-monitor\
    description: >\
      Medical safety agent focused on detecting dangerous or emergency-level\
      blood pressure values and determining the appropriate urgency level.\
    model: claude-sonnet-4-20250514\
    temperature: 0.2\
    max_tokens: 1000\
    function: |\
      - Identify hypertensive crisis (\uc0\u8805 180/\u8805 120)\
      - Detect stage 2 hypertension and persistent elevation\
      - Flag rapid spikes or extreme variability\
      - Produce safety score and alert level\
      - Interrupt workflow if emergency is detected\
    tools_used:\
      - threshold_monitoring\
      - emergency_detection\
      - risk_escalation_logic\
      - safety_recommendation_engine\
\
  - name: bp-nutrition-coach\
    description: >\
      Nutrition & lifestyle recommendation agent providing personalized,\
      evidence-based dietary strategies to help lower blood pressure.\
    model: claude-sonnet-4-20250514\
    temperature: 0.7\
    max_tokens: 3000\
    function: |\
      - Interpret BP analysis and safety results\
      - Generate dietary recommendations based on DASH and other evidence\
      - Provide food lists (embrace vs avoid)\
      - Prioritize interventions by impact and safety\
      - Produce motivational guidance and estimated BP reduction impact\
    tools_used:\
      - dash_diet_knowledge_base\
      - nutritional_intervention_planning\
      - personalized_recommendation_engine\
      - food_category_classifier\
      - behavior_change_guidance\
\
team_workflow:\
  name: bp-health-companion-team\
  description: Multi-agent sequential workflow analyzing BP readings and generating recommendations.\
  type: sequential\
  stages:\
    - stage: safety_monitoring\
      agent: bp-safety-monitor\
      order: 1\
      stop_on_emergency: true\
      timeout: 30s\
\
    - stage: data_analysis\
      agent: bp-data-analyzer\
      order: 2\
      depends_on: safety_monitoring\
      timeout: 45s\
\
    - stage: nutrition_guidance\
      agent: bp-nutrition-coach\
      order: 3\
      depends_on:\
        - safety_monitoring\
        - data_analysis\
      timeout: 60s\
\
output_schema:\
  aggregation: combined_json\
  final_structure:\
    safety: "Output of bp-safety-monitor"\
    analysis: "Output of bp-data-analyzer"\
    recommendations: "Output of bp-nutrition-coach"\
    metadata:\
      analyzedAt: timestamp\
      agentsUsed: list\
      executionTime: ms\
}